# Copy this file to .env and fill values
FLASK_ENV=development
FLASK_DEBUG=1
SECRET_KEY=change-me

# Storage
UPLOAD_DIR=/workspace/storage/uploads
TRANSCRIPTS_DIR=/workspace/storage/transcripts
RESULTS_DIR=/workspace/storage/results

# LLM Providers
MISTRAL_API_KEY=
MISTRAL_MODEL=open-mixtral-8x7b
OPENAI_API_KEY=
USE_OPENAI_WHISPER_API=false

# AWS (optional)
AWS_REGION=
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_S3_BUCKET=
ENABLE_S3=false